{
  "@std/esm": {
    "esm": "js"
  },
  "main": "./src/server/index.js",
  "name": "boilerplate-nutella",
  "version": "1.0.0",
  "description": "SSR plus Webpack ready.",
  "scripts": {
    "start": "./node_modules/.bin/razzle start",
    "build": "./node_modules/.bin/razzle build",
    "start:prod": "cross-env NODE_ENV=production node build/server.js",
    "production": "npm run build && npm run start:prod",
    "lint": "eslint common server client --ignore-pattern __tests__",
    "test": "cross-env NODE_ENV=test ava --verbose"
  },
  "dependencies": {
    "@std/esm": "^0.17.3",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cookies": "^0.7.1",
    "cookies-js": "^1.2.3",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "moment": "^2.18.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-cookies": "^1.0.1",
    "redux-thunk": "^2.2.0",
    "revalidator": "^0.3.1",
    "string-mask": "^0.3.0",
    "sweetalert2": "^7.9.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-ava": "^2.4.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jsdom": "^11.2.0",
    "nyc": "^11.2.1",
    "razzle": "^0.8.11",
    "react-hot-loader": "^3.1.3",
    "redux-ava": "^2.2.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "require": [
      "./src/util/test-server-setup.js",
      "babel-register"
    ],
    "babel": "inherit"
  }
}
